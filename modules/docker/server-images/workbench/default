
	upstream docker-virtue-admin {
		server virtue-admin:8080;
	}

	server {
		listen 8080 default_server;
		listen [::]:8080 default_server;

		root /usr/share/nginx/html;

		index index.html;

		server_name _;

		location / {
			try_files $uri $uri/ =404;
		}

		location /java {
			rewrite ^/java(.*) $1 break;
			proxy_pass "http://docker-virtue-admin";
		}
	}
